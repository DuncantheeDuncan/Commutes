  <ul class="people_list">
  <li><h1>Leader</h1></li>
    {{#each user.leader}}
      <li>{{this}}</li>
    {{/each}}
  </ul>

  <ul class="people_list">
  <li><h1>Start from</h1></li>
      {{#each user.origin}}
        <li>{{this}}</li>
      {{/each}}
    </ul>

<ul class="people_list">
  <li><h1>Destination</h1></li>
      {{#each user.destination}}
        <li>{{this}}</li>
      {{/each}}
    </ul>

<ul class="people_list">
  <li><h1>Departure time</h1></li>
      {{#each user.departure}}
        <li>{{this}}</li>
      {{/each}}
    </ul>
<ul class="people_list">
  <li><h1>Pool created at</h1></li>
      {{#each user.created_at}}
        <li>{{this}}</li>
      {{/each}}
    </ul>


